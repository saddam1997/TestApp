<h3>Customers</h3>
<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>State</th>

  </tr>
  <%_.each(customers,function (customer) { %>
 <tr data-id="<%=customer.id%>" data-model="customer">
  <td><%=customer.id%></td>
  <td><%=customer.name%></td>
  <td><%=customer.email%></td>
  <td><a href="/customer/show/<%=customer.id%>">show</a></td>
  <td><a href="/customer/edit/<%=customer.id%>">edit</a></td>
  <td><a href="/customer/destroy/<%=customer.id%>">destroy</a></td>
 </tr>
  <% })%>


</table>
